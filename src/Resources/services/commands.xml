<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd"
>
    <parameters>
        <parameter key="mycustombundle.command.generate_products.class">BitBag\SyliusVueStorefront2Plugin\Command\GenerateProductsCommand</parameter>
    </parameters>

    <services>
        <service id="bitbag.sylius_vue_storefront2_plugin.command.generate_products"
                 class="BitBag\SyliusVueStorefront2Plugin\Command\GenerateProductsCommand">
            <argument type="service" id="sylius.repository.product" />
            <argument type="service" id="sylius.factory.product" />
            <argument type="service" id="doctrine.orm.entity_manager" />
            <argument type="service" id="sylius.repository.taxon" />
            <argument type="service" id="sylius.factory.product_variant" />
            <argument type="service" id="sylius.repository.channel" />
            <argument type="service" id="sylius.factory.channel_pricing" />
            <argument type="service" id="sylius.repository.product_taxon" />
            <tag name="console.command" />
        </service>
    </services>
</container>
